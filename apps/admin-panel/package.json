    "@types/lodash": "^4.17.20",
    "@types/node": "20",
    "@types/react": "19.2.2",
    "@types/react-dom": "19.2.2",
    "@typescript-eslint/eslint-plugin": "^7.12.0",
    "cypress": "^15.6.0",
    "cypress-file-upload": "^5.0.8",
    "eslint": "^9.38.0",
