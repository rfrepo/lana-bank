    "keycloak-js": "^26.2.1",
    "lucide-react": "^0.552.0",
    "next": "16.0.1",
    "next-intl": "^4.4.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-idle-timer": "^5.7.2",
